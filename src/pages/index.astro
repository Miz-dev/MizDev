---
import BaseLayout from "../layouts/BaseLayout.astro";
const pageTitle = "MizLog Astro";
---

<BaseLayout pageTitle={pageTitle}>
  <h2>My awesome blog subtitle</h2>
</BaseLayout>

<style>
  li {
    list-style: none;
  }
  li a {
    color: #2c568e;
    text-decoration: none;
    transition: all 0.4s;
  }
  li a:hover,
  a:focus {
    opacity: 0.7;
  }
</style>
